% Online ресурм

@Online{leinster,
    author = {Tom Leinster},
    title = {Rethinking set theory},
    date = {2012-12-28},
    _url = {http://arxiv.org/abs/1212.6543v1},
    language = {english},
    langid = {english},
    annote = {Mathematicians manipulate sets with confidence almost every day, rarely
    making mistakes. Few of us, however, could accurately quote what are
    often referred to as ”the” axioms of set theory. This suggests that we
    all carry around with us, perhaps subconsciously, a reliable body of
    operating principles for manipulating sets. What if we were to take some
    of those principles and adopt them as our axioms instead? The message of
    this article is that this can be done, in a simple, practical way (due
    to Lawvere). The resulting axioms are ten thoroughly mundane statements
    about sets. This is an expository article for a general mathematical
    readership.},
    eprint = {1212.6543v1},
    eprinttype = {arxiv},
    eprintclass = {math.LO},
}